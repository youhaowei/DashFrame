{
  "name": "@dashframe/connector-local",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist *.tsbuildinfo",
    "dev": "tsc -w -p tsconfig.json --preserveWatchOutput",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "@dashframe/csv": "workspace:*",
    "@dashframe/json": "workspace:*",
    "@dashframe/engine": "workspace:*",
    "@dashframe/engine-browser": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.6.0",
    "vitest": "^3.2.4"
  }
}
